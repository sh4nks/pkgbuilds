pkgname=cinnamon-theme-void
pkgver=1.0
pkgrel=1
pkgdesc="Void Cinnamon theme"
arch=('any')
url="http://cinnamon-spices.linuxmint.com/themes/view/104"
license=('unknown')
depends=('cinnamon')
source=("http://cinnamon-spices.linuxmint.com/uploads/themes/8E6A-1I0P-X06K.zip")
md5sums=('bed3bbae3ac0da757992180ae2edbb92')

package() {
  find VOID -type f -not -name *~ -exec install -Dm644 '{}' "$pkgdir/usr/share/themes/{}" \;
}
