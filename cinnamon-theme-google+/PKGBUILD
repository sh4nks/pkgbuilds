pkgname=cinnamon-theme-google+
pkgver=1.6
pkgrel=1
pkgdesc="Google+ theme for cinnamon"
arch=('any')
url="http://cinnamon-spices.linuxmint.com/themes/view/89"
license=('unknown')
depends=('cinnamon')
source=("http://cinnamon-spices.linuxmint.com/uploads/themes/I74P-XVMO-7663.zip")
md5sums=('a3a0e26e920b9cdbb163c0fca910be52')

package() {
  for i in Google+*; do
     find "$i" -type f -not -name *~ -exec install -Dm644 '{}' 
"$pkgdir/usr/share/themes/{}" \;
  done
}
